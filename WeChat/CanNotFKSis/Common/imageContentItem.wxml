<!--Common/imageContentItem.wxml-->
<view class='item_container'>
    <view class='noraml_container'>
        <image class='icon_image' src='{{image}}'></image>
        <text class='item_content'>{{content}}</text>
    </view>
    <image class='item_next_image' wx:if="{{showNext}}" bindtap='onClicked' src='{{nextImage}}'></image>
    <slot/>
</view>