<!--pages/test/media/media.wxml-->
<view class="container">
  <image src="{{poster}}"></image>
  <audio poster="{{poster}}" name="{{name}}" author="{{author}}" src="{{src}}" id="myAudio" controls loop></audio>
  <video poster="{{poster}}" src="{{vedioSrc}}" autoplay="true" muted="true" danmu-list="{{danmuList}}" danmu-btn="true" enable-danmu="true"></video>
</view>