<!--pages/user/userTemplate/userItem.wxml-->
<template name="userItem">
    <view class='item_container' bindtap='compEvent'>
        <image class='item_icon'>{{image}}</image>
        <text class='item_text'>{{text}}</text>
        <text class='item_next' wx:if="{{isNextHide}}">{{next}}</text>
    </view>
</template>
